import java.util.*;
class  Duplicate_Remove_String

{
	public static void main(String[] args) 
	{
		String s="bananas";
		linkedHashSet lhs=new LinkedHashSet();
		for(int i=0;i<s.length();i++)
			{
		char c=s.charAt(i);
		lhs.add(c);
		}
		String t=""+lhs;
		t=t.replaceAll("[,]","");
		t=t.replace("]","");
		t=t.replace("[","");
		System.out.println(t);
	}
}
